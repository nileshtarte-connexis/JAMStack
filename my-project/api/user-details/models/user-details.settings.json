    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "phoneNumber": {
      "type": "biginteger",
      "unique": true,
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "string",
      "required": true
    }
  }
}
